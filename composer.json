{
	"name": "nielslange/smntcs-deactivate-image-compression",
	"type": "plugin",
	"description": "Deactivate the WordPress default image compression which got introduced with WordPress 2.5.",
	"homepage": "https://github.com/nielslange/smntcs-deactivate-image-compression",
	"support": {
		"issues": "https://github.com/nielslange/smntcs-deactivate-image-compression/issues",
		"source": "https://github.com/nielslange/smntcs-deactivate-image-compression"
	},
	"keywords": [
		"WordPress",
		"Plugin",
		"SMNTCS Deactivate image compression"
	],
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Niels Lange",
			"email": "info@nielslange.de"
		}
	],
	"require": {
		"php": ">=7.0"
	},
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
		"humanmade/psalm-plugin-wordpress": "^2.0",
		"overtrue/phplint": "^3.0",
		"phpcompatibility/phpcompatibility-wp": "^2.1",
		"squizlabs/php_codesniffer": "^3.6",
		"vimeo/psalm": "^4.7",
		"wp-coding-standards/wpcs": "^3.0"
	},
	"scripts": {
		"phpcbf": "phpcbf --standard=phpcs.dist.xml .",
		"phpcs": "phpcs --standard=phpcs.dist.xml .",
		"phplint": "phplint"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
